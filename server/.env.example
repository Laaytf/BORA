# Exemplo de arquivo .env para o webhook
# Copie este arquivo para .env e preencha com suas credenciais

# Credenciais do Supabase
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Porta do webhook (padrão: 3001)
WEBHOOK_PORT=3001

# Token secreto para autenticação (opcional, mas recomendado)
WEBHOOK_SECRET_TOKEN=seu-token-super-secreto-aqui

# Ambiente (development ou production)
NODE_ENV=development
